{
  "name": "prism-neuro-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:clean && npm run build:tsc ",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "build:clean": "rm -r dist; exit 0",
    "tsc": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:backend": "ts-node-dev ./src/apps/prism-neuro-backend/backend/start.ts",
    "start:backend": "ts-node-dev dist/src/apps/prism-neuro-backend/backend/start",
    "migration:create": "prisma migrate dev --create-only",
    "migration:dev": "prisma migrate dev",
    "migration:status": "prisma migrate status",
    "migration:deploy": "prisma migrate deploy",
    "generate:prisma": "prisma generate",
    "swagger": "ts-node-dev src/contexts/shared/infrastructure/Swagger/swagger.ts",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^53.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "prisma": "^5.16.1",
    "swagger-autogen": "^2.23.7",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "awilix": "^10.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "swagger-ui-express": "^5.0.1"
  }
}
